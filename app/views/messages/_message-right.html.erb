<div class="messages-container-right">
  <div class="avatar-chat">
    <%= cl_image_tag message.user.photo.key, class: "avatar-small"%>
  </div>
  <div>
    <div  class="messages-bubble-right">
      <div id="message-<%= message.id %>">
        <div class="message-content">
          <p><%= message.content %></p>
        </div>
      </div>
    </div>
    <p class="paragraph-chat-time"><%= message.created_at.strftime("%a %b %e at %l:%M%p") %></p>
  </div>
</div>
